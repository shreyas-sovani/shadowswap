# ShadowSwap Contract Deployment Configuration
# Copy this file to .env and fill in the values

# Sepolia RPC URL (get from Alchemy, Infura, or similar)
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_API_KEY

# Private key for deployment (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Solver address (backend wallet that will execute swaps)
SOLVER_ADDRESS=0xD2aA21AF4faa840Dea890DB2C6649AACF2C80Ff3

# Optional: Etherscan API key for verification
ETHERSCAN_API_KEY=your_etherscan_api_key
